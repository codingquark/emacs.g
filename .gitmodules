[borg]
	pushDefault = locutus
	collective = emacsg
[submodule "ag"]
	path = lib/ag
	url = https://github.com/Wilfred/ag.el
[submodule "alert"]
	path = lib/alert
	url = https://github.com/jwiegley/alert
[submodule "auctex"]
	path = lib/auctex
	url = git@github.com:emacsmirror/auctex.git
	load-path = .
	build-step = ./autogen.sh
	build-step = ./configure --with-lispdir=~/.emacs.d/lib/auctex
	build-step = make
	build-step = make doc
	build-step = borg-maketexi
	build-step = borg-makeinfo
	build-step = borg-update-autoloads
[submodule "auto-compile"]
	path = lib/auto-compile
	url = git@github.com:emacscollective/auto-compile.git
